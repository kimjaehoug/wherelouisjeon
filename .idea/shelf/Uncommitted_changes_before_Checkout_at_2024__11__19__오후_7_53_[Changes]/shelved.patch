@@ -0,0 +1,147 @@
+package kr.jbnu.se.std;
+
+import javax.sound.sampled.*;
+import java.awt.*;
+import java.io.File;
+import java.io.IOException;
+import java.util.ArrayList;
+import java.util.List;
+
+public class BossManager {
+    private List<boss1> bossList;
+    private Clip clip;
+    private List<BossAttack> bossAttacks;
+    private long lastBossAttackTime;
+    private Framework framework;
+    private int bossAttackInterval;
+    private int playerHp;
+
+    public BossManager(Framework framework, int initialPlayerHp, int attackInterval) {
+        this.framework = framework;
+        this.playerHp = initialPlayerHp;
+        this.bossAttackInterval = attackInterval;
+
+        bossList = new ArrayList<>();
+        bossAttacks = new ArrayList<>();
+    }
+
+    // 보스 업데이트 및 공격 생성
+    public void updateBossActions(Point mousePosition, int round) {
+        for (boss1 boss : bossList) {
+            boss.update();
+            generateRoundAttack(round, boss, mousePosition);
+            updateAndCheckAttack(bossAttacks, mousePosition, round);
+        }
+    }
+
+    private void generateRoundAttack(int round, boss1 boss, Point mousePosition) {
+        if (System.nanoTime() - lastBossAttackTime >= bossAttackInterval * 900_000) {
+            double gravity = 6;
+            double speed = 150;
+            double deltaTime = 0.1;
+
+            switch (round) {
+                case 1 -> generateAngleAttacks(boss, 3, 15);
+                case 2 -> generateGravityAttacks(boss, 4, speed, gravity, deltaTime);
+                case 3, 4, 5 -> {
+                    generateHomingAttacks(boss, 4, speed, gravity, deltaTime, mousePosition);
+                    if (round >= 4) {
+                        int bottomAttacks = round == 4 ? 3 : 5;
+                        generateBottomAttacks(bottomAttacks, speed, gravity, deltaTime);
+                    }
+                }
+            }
+            // 보스 공격 사운드 재생
+            playActiveSound("src/main/resources/sounds/bossattck.wav");
+            lastBossAttackTime = System.nanoTime();
+        }
+    }
+
+    // 라운드 1: 일정 각도로 발사되는 공격
+    private void generateAngleAttacks(boss1 boss, int numAttacks, int speed) {
+        for (int i = 0; i < numAttacks; i++) {
+            double angle = 150 + Math.random() * 70;
+            bossAttacks.add(new BossAttack(boss.x, boss.y, angle, speed));
+        }
+    }
+
+    // 라운드 2: 중력 적용 공격
+    private void generateGravityAttacks(boss1 boss, int numAttacks, double speed, double gravity, double deltaTime) {
+        for (int i = 0; i < numAttacks; i++) {
+            double angle = Math.toRadians(150 + Math.random() * 70);
+            double vx = speed * Math.cos(angle);
+            double vy = speed * Math.sin(angle);
+            bossAttacks.add(new BossAttack(boss.x, boss.y, vx, vy, gravity, deltaTime));
+        }
+    }
+
+    // 라운드 3, 4, 5: 마우스를 향한 유도 공격
+    private void generateHomingAttacks(boss1 boss, int numAttacks, double speed, double gravity, double deltaTime, Point mousePosition) {
+        for (int i = 0; i < numAttacks; i++) {
+            double angle = Math.toRadians(150 + Math.random() * 70);
+            double vx = speed * Math.cos(angle);
+            double vy = speed * Math.sin(angle);
+            bossAttacks.add(new BossAttack(boss.x, boss.y, vx, vy, gravity, deltaTime));
+        }
+        double homingAngle = Math.atan2(mousePosition.getY() - boss.y, mousePosition.getX() - boss.x);
+        double vxHoming = speed * Math.cos(homingAngle);
+        double vyHoming = speed * Math.sin(homingAngle);
+        bossAttacks.add(new BossAttack(boss.x, boss.y, vxHoming, vyHoming, gravity, deltaTime));
+    }
+
+    // 라운드 4, 5: 화면 아래에서 위로 발사되는 공격
+    private void generateBottomAttacks(int numAttacks, double speed, double gravity, double deltaTime) {
+        double upwardAngle = Math.toRadians(80 + Math.random() * 10);
+        double vxUp = speed * Math.cos(upwardAngle);
+        double vyUp = -speed * Math.sin(upwardAngle);
+        double bottomY = framework.getHeight();
+
+        for (int i = 0; i < numAttacks; i++) {
+            double startX = framework.getWidth() * (0.1 + Math.random() * 0.3);
+            bossAttacks.add(new BossAttack((int) startX, (int) bottomY, vxUp, vyUp, gravity, deltaTime));
+        }
+    }
+
+    // 공격 업데이트 및 피격 체크 로직을 통합
+    private void updateAndCheckAttack(List<BossAttack> attacks, Point mousePosition, int round) {
+        for (int i = 0; i < attacks.size(); i++) {
+            BossAttack attack = attacks.get(i);
+            if (round >= 2) attack.updatewithgravity();
+            else attack.update();
+
+            // 플레이어와 충돌 확인
+            if (attack.isHit(mousePosition)) {
+                System.out.println("Player hit! Remaining health: ");
+                playerHp -= 10;
+                playActiveSound("src/main/resources/sounds/hit.wav"); // 피격 사운드 재생
+                attacks.remove(i--);
+            } else if (attack.isOutOfScreen(framework)) {
+                attacks.remove(i--);
+            }
+        }
+    }
+
+    public int getPlayerHp() {
+        return playerHp;
+    }
+
+    public void addBoss(boss1 newBoss) {
+        bossList.add(newBoss);
+    }
+
+    public void clearBossAttacks() {
+        bossAttacks.clear();
+    }
+
+    // 사운드 재생 메서드
+    private void playActiveSound(String filePath) {
+        try{
+            AudioInputStream audioStream = AudioSystem.getAudioInputStream(new File(filePath));
+            clip = AudioSystem.getClip();
+            clip.open(audioStream);
+            clip.start();
+        }catch(UnsupportedAudioFileException | IOException | LineUnavailableException e) {
+            e.printStackTrace();
+        }
+    }
+}
